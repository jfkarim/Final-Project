<ul>
  <% (user.events - user.pending_events).each do |event| %>
    <% eu = EventUser.find_by_event_id_and_user_id(event.id, user.id) %>
    <li value="<%= event.id %>">
      <%= link_to event.name, event_url(event) %><br>
      <%= render "event_users/status", event: event, user: user %>
    </li>
  <% end %>
</ul>