<form
  action="<%= user_photos_url(user) %>"
  class="lone-upload"
  method="post"
  enctype="multipart/form-data">
  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>">
  <input type="file" name="photo[picture]">
  <input type="submit" value="Add Photo">
</form>